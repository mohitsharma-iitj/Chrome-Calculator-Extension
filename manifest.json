{
  "manifest_version": 3,
  "name": "Mohit's Extension",
  "version": "1.0",
  "description": "Use the action icon or keyboard shortcut to open the side panel on google.com",
  "background": {
        "service_worker": "service-worker.js"
    },

  "action": {
    "default_title": "Click Me"
  },
  "permissions": ["sidePanel", "tabs"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}